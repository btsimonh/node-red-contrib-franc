{
  "name": "node-red-contrib-franc",
  "version": "0.0.1",
  "description": "A language guessing node for node-red",
  "scripts": {
    "prepublish": "babel --copy-files --out-dir franc src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jemagu/node-red-contrib-franc.git"
  },
  "dependencies": {
    "franc": "1.1.1"
  },
  "keywords": [
    "node-red", "language identification", "language guessing"
  ],
  "node-red": {
    "nodes": {
      "franc": "franc/franc.js"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18"
  }
}
